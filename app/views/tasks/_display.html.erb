<div class ='well' > 
    <p>
      <b>Task id:</b>
      <%= params[:id] %>
    </p>

    <p>
      <b>Title:</b>
      <%= task.title %>
    </p>

    <p>
      <b>Description:</b>
      <%= task.description %>
    </p>

    <p>
      <b>Status:</b>
      <%= task.status %>
    </p>

    <p>
      <b>Due date:</b>
      <%= task.due_date %>
    </p>
  

    <%= link_to 'Edit', edit_project_task_path(task.project,task.id) %> |
    <%= link_to 'Back', project_path(task.project)%>
</div>